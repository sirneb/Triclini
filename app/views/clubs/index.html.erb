<h1>Listing Clubs</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Address</th>
  </tr>

<% @clubs.each do |club| %>
  <tr>
    <td><%= link_to club.name, root_url(subdomain: club.subdomain) %></td>
    <td><%= club.address %></td>
    <td><%= link_to 'Edit', edit_club_path(club) %></td>
    <td><%= link_to 'Destroy', club, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Club', new_club_path %>
